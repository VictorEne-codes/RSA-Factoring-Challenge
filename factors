#!/usr/bin/python3
import sys
import math

def factorize(number):
    # Start from the largest possible factor (number // 2) and go down to 2
    for i in range(number // 2, 1, -1):
        if number % i == 0:
            return i, number // i
    return 1, number  # Fallback if no factors found, should not occur since n > 1

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        return

    filename = sys.argv[1]

    try:
        with open(filename, 'r') as file:
            numbers = file.readlines()
    except FileNotFoundError:
        print(f"Error: File {filename} not found")
        return

    for line in numbers:
        number = int(line.strip())
        p, q = factorize(number)
        print(f"{number}={p}*{q}")

if __name__ == "__main__":
    main()
